<Window x:Class="SongTheoryApplication.Views.Windows.NotImplementedActionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SongTheoryApplication.Views.Windows"
        xmlns:vm2="clr-namespace:SongTheoryApplication.ViewModels.Windows"
        d:DataContext="{d:DesignInstance vm2:NotImplementedActionWindowViewModel}"
        mc:Ignorable="d"
        Title="Akce není implementována"
        Height="{Binding Path=Height, Mode=TwoWay}" Width="{Binding Path=Width, Mode=TwoWay}"
        MinWidth="{Binding Path=MinWidth, Mode=OneWay}" MinHeight="{Binding Path=MinHeight, Mode=OneWay}"
        Left="{Binding Path=LeftWindow, Mode=OneWay}" Top="{Binding Path=TopWindow, Mode=OneWay}"
        ResizeMode="NoResize">
    <Grid>
        <TextBlock HorizontalAlignment="Center"
                   VerticalAlignment="Center">
            Akce, kterou chcete provést není implementována.
        </TextBlock>
    </Grid>
</Window>
